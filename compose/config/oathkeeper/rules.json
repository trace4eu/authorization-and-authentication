[
  {
    "id": "allow-anonymous-with-header-mutator",
    "upstream": {
      "url": "http://hydra:4445"
    },
    "match": {
      "url": "http://localhost:4445/<.*>",
      "methods": ["GET","POST"]
    },
    "authenticators": [
      {
        "handler": "jwt"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [{ "handler": "noop" }]
  }
]
